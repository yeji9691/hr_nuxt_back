<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.seoulit.erp.hr.base.dao.WorkplaceListDAO">


	<resultMap type="kr.co.seoulit.erp.hr.company.to.WorkplaceTO" id="workplaceResult">
		<result property="workplaceCode" column="WORKPLACE_CODE" />
		<result property="workplaceName" column="WORKPLACE_NAME" />
	</resultMap>

	<select id="findWorkplaceList" resultMap="workplaceResult">
		SELECT WORKPLACE_CODE, WORKPLACE_NAME
		FROM WORKPLACE
		WHERE COMPANY_CODE='COM-01'
	</select>
</mapper>
